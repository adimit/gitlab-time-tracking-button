{
  "extends": [
    "airbnb"
  ],
  "env": {
    "mocha": true,
    "browser": true
  },
  "plugins": [
    "chai-friendly"
  ],
  "globals": {
    "chrome": true,
    "expect": true,
    "assert": true,
    "sinon": true
  },
  "rules": {
    "no-unused-expressions": 0,
    "chai-friendly/no-unused-expressions": 2
  }
}
