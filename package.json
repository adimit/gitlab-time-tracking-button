{
  "name": "gitlab-time-tracking-button",
  "version": "1.0.5",
  "description": "Adds a time tracking button to Gitlab's issue interface",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build:firefox": "TARGET=firefox VERSION=\"$npm_package_version\" webpack && rm -f addon/manifest.js && find addon -name '*.map' -exec rm -f '{}' ';'",
    "build:chromium": "TARGET=chromium VERSION=\"$npm_package_version\" webpack && rm -f addon/manifest.js && find addon -name '*.map' -exec rm -f '{}' ';'",
    "bundle": "web-ext -s ./addon -a . build --overwrite-dest",
    "clean": "rm -rf addon/*",
    "lint": "eslint src --ext .js,.json",
    "lint:fix": "npm run lint -- --fix",
    "start": "concurrently --kill-others \"npm run watch:build\" \"npm run webext:run\"",
    "test": "karma start --browsers ChromiumHeadless --single-run=true",
    "watch:build": "TARGET=firefox VERSION=\"$npm_package_version\" webpack  --progress --colors --watch",
    "watch:test": "karma  start --browsers ChromiumHeadless --single-run=false",
    "watch:debug": "karma start --browsers Chromium --single-run=false",
    "webext:lint": "web-ext -s ./addon lint",
    "webext:run": "web-ext -s ./addon run --verbose"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.0.2",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-chai-friendly": "^0.4.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^1.7.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-coveralls": "^1.1.2",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chrome": "^0.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.5",
    "selenium-webdriver": "^3.4.0",
    "sinon": "^3.2.0",
    "sinon-chrome": "^2.2.1",
    "sinon-test": "^2.1.0",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "virtualenv": "^0.3.1",
    "web-ext": "^2.0.0",
    "webpack": "^3.5.3"
  },
  "dependencies": {
    "feather-icons": "^3.2.2"
  },
  "permissions": {
    "multiprocess": true
  }
}
